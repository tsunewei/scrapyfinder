CREATE TABLE ptttitle (
  id int(11) NOT NULL AUTO_INCREMENT,
  date char(5) COLLATE utf8_unicode_ci DEFAULT NULL,
  push char(3) COLLATE utf8_unicode_ci DEFAULT NULL,
  title char(254) COLLATE utf8_unicode_ci DEFAULT NULL,
  url char(254) COLLATE utf8_unicode_ci DEFAULT NULL,
  auth char(254) COLLATE utf8_unicode_ci DEFAULT NULL,
  systime datetime NULL DEFAULT NULL,
  PRIMARY KEY (id)
) COLLATE='utf8_general_ci' ENGINE=InnoDB;
